									"\r",
									"    pm.test(\"Booking total price in response matches the expected value: \" + pm.environment.get(\"total_price\"), function () {\r",
									"        pm.expect(Number(pm.environment.get(\"total_price\"))).to.eql(responseData.totalprice);\r",
									"    });\r",
									"\r",
									"    pm.test(\"Booking deposit status in response matches the expected value: \" + pm.environment.get(\"deposit_paid_status\"), function () {\r",
