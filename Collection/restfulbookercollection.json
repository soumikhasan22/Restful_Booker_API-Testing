				{
					"name": "create_booking",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var firstname=pm.variables.replaceIn(\"{{$randomFirstName}}\");\r",
									"pm.environment.set(\"first_name\", firstname);\r",
									"\r",
									"var lastname=pm.variables.replaceIn(\"{{$randomLastName}}\");\r",
									"pm.environment.set(\"last_name\", lastname);\r",
									"\r",
									"var totalprice=pm.variables.replaceIn(\"{{$randomInt}}\");\r",
									"pm.environment.set(\"total_price\", totalprice);\r",
									"\r",
									"var depos=pm.variables.replaceIn(\"{{$randomBoolean}}\");\r",
									"pm.environment.set(\"deposit_paid_status\", depos);\r",
									"\r",
									"const checkindate=require('moment');\r",
									"const check_in_date=checkindate();\r",
									"var checkInDate=check_in_date.format(\"YYYY-MM-DD\")\r",
									"pm.environment.set(\"check_in_date\", checkInDate);\r",
									"\r",
									"const checkoutdate=require('moment');\r",
									"const check_out_date=checkindate();\r",
									"var checkoutDate=check_in_date.add(5,'d').format(\"YYYY-MM-DD\")\r",
									"pm.environment.set(\"check_out_date\", checkoutDate);\r",
									"\r",
									"var need=[\"Breakfast\", \"Lunch\", \"Dinner\"]\r",
									"var additionalneeds = need[Math.floor(Math.random() * need.length)];\r",
									"pm.environment.set(\"additional_needs\", additionalneeds);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"\r",
									"var status_code = pm.response.code;\r",
									"\r",
									"\r",
									"if (status_code === 200) {\r",
									"    var responseData = pm.response.json();\r",
									"    pm.test(\"Booking completed successfully! Your Booking ID is: \" + responseData.bookingid);\r",
									"    pm.environment.set(\"booking_id\", responseData.bookingid);\r",
									"} \r",
									"else if (status_code === 400) {\r",
									"    pm.test(\"Booking failed: Invalid input provided\");\r",
									"} \r",
									"else if (status_code === 404) {\r",
									"    pm.test(\"Booking failed: The requested endpoint was not found\");\r",
									"} \r",
									"else {\r",
									"    pm.test(\"Booking failed: Unexpected status code received (\" + status_code + \")\");\r",
									"}\r",
